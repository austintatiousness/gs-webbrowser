{
  "name": "webview",
  "description": "webview",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "webview",
    {"fileSystem": ["read"]}
  ],
  "offline_enabled": true,
  "file_handlers": {
    "any": {
      "extensions": ["pid"]
    }
  },
  "sockets": {
    "tcp": {
      "connect": ""
    },
    "tcpServer": {
      "listen": ":2222"
    }
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": { "16": "icon.jpg", "128": "icon.jpg"}
}
